#include <SoftwareSerial.h>

#define TXPIN 2
#define RXPIN 3

SoftwareSerial mySerial(RXPIN, TXPIN);

byte packet[44] = {0xC0, 0x00, 0xAA, 0xA2, 0xA4, 0xAC, 0xAA, 0xA2, 0x60, 0x9A, 0x60, 0x98, 0xA8, 0x8A, 0x40, 0xEA, 0xAE, 0x92, 0x88, 0x8A, 0x62, 0x40, 0x62, 0xAE, 0x92, 0x88, 0x8A, 0x64, 0x40, 0x65, 0x03, 0xF0, 0x27, 0x77, 0x59, 0x44, 0x6C, 0x20, 0x1C, 0x5B, 0x2F, 0x3E, 0x0D, 0xC0};

void setup() {
  mySerial.begin(38400);
  //Serial.begin(38400);
  pinMode(LED_BUILTIN, OUTPUT);  
}

void loop() {
  
  digitalWrite(LED_BUILTIN, HIGH);
  delay(200);               // wait for a second
  
  digitalWrite(LED_BUILTIN, LOW); 
  
  for (int i=0; i<44; i++){
    mySerial.write(packet[i]);
  }

  delay(5000);
}
